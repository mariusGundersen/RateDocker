@model RateDocker.Models.Votes
@{
    ViewData["Title"] = "Results";
}
<h2>@ViewData["Title"]</h2>


<div class="form-horizontal">
  @foreach(var vote in Model.Results){
    <div class="form-group">
      <label class="col-sm-3 control-label">@vote.Name</label>
      <div class="col-sm-9">
        <div class="form-control-static">
          <div class="progress">
            <div class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="min-width: 2em; width:@((int)(vote.Percentage))%">
              @vote.Count
            </div>
          </div>
        </div>
      </div>
    </div>
  }
</div>